<?xml version='1.0' encoding='utf-8'?>
<widget xmlns:gap="http://phonegap.com/ns/1.0" id="com.designge.priustaxi" package="com.designge.priustaxi"
        version="1.0.7" versionCode="23" xmlns="http://www.w3.org/ns/widgets">
  <name>Prius Taxi</name>
  <!--<icon src="www/taxi.png"/>-->
  <platform name="android">
    <preference name="android-minSdkVersion" value="22"/>
    <allow-intent href="market:*"/>
  </platform>
  <access origin="*"/>
  <allow-intent href="*"/>
  <allow-navigation href="*"/>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <preference name="target-device" value="handset"/>
  <preference name="phonegap-version" value="cli-7.0.1"/>
  <preference name="orientation" value="portrait"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="fullscreen" value="true"/>
  <preference name="keepRunning" value="true"/>
  <preference name="exit-on-suspend" value="false"/>
  <description>
    Prius Taxi Tbilisi
  </description>
  <author email="uabashidze@gmail.com" href="http://design.ge">
    Cyborg-Mcloud
  </author>
  <plugins>
  </plugins>
  <feature name="http://api.phonegap.com/1.0/device"/>
  <feature name="http://api.phonegap.com/1.0/geolocation"/>
  <feature name="http://api.phonegap.com/1.0/network"/>
  <feature name="http://api.phonegap.com/1.0/notification"/>
  <feature name="Notification">
    <param name="android-package" value="org.apache.cordova.dialogs.Notification"/>
    <param name="ios-package" value="CDVNotification"/>
  </feature>
  <feature name="Geolocation">
    <param name="android-package" value="org.apache.cordova.GeoBroker"/>
  </feature>
  <feature name="App">
    <param name="android-package" value="com.designge.locage"/>
  </feature>
  <gap:plugin name="cordova-plugin-whitelist" source="npm"/>
  <gap:plugin name="cordova-plugin-powermanagement" source="npm"/>
  <plugin name="cordova-plugin-geolocation" source="npm"/>
  <gap:plugin name="cordova-plugin-geolocation" source="npm"/>
  <gap:plugin name="cordova-plugin-file" source="npm"/>
  <gap:plugin name="cordova-plugin-network-information" source="npm"/>
  <gap:plugin name="de.appplant.cordova.plugin.local-notification" source="npm" version="0.8.4"/>
  <preference name="Orientation" value="portrait"/>
  <engine name="browser" spec="~5.0.1"/>
  <plugin name="cordova-plugin-network-information" spec="~1.3.4"/>
  <engine name="android" spec="~6.3.0"/>
  <engine name="ios" spec="~4.5.4"/>
</widget>
